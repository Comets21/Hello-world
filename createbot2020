int main()
{
    create_connect();
    create_drive_direct(100, 100);
    msleep(2500);
    create_drive_direct(-100, 100);
    msleep(1640);
    create_drive_direct(100, 100);
    msleep(6000);
    create_disconnect();
    enable_servos();
    set_servo_position(0, 124);
    create_connect();
    create_drive_direct(-100, -100);
    msleep(6000);
    return 0;
}



